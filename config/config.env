# common
rootNamespace=cf101

# api
externalFQDN=api.example.org
internalPort=9000
lifecycleType=buildpack
lifecycleStack=cflinuxfs3
lifecycleStagingMemoryMB=1024
lifecycleStagingDiskMB=1024
packageRegistryBase=gcr.io/cf-relint-greengrass/korifi-controllers/kpack/beta
packageRegistrySecret=image-registry-credentials
defaultDomainName=apps.example.org

# controllers
buildReconciler=kpack-image-builder
appReconciler=statefulset-runner
processDefaultMemoryMB=1024
processDefaultDiskQuotaMB=1024
packageRegistrySecretName=image-registry-credentials
taskTTL=30d
workloads_tls_secret_name=korifi-workloads-ingress-cert
workloads_tls_secret_namespace=korifi-controllers-system
